include Make.inc

obj = ATL_cnrm2.o ATL_cnrm2_xp0yp0aXbX.o

all : lib
clib : lib
lib : cnrm2.grd

cnrm2.grd : $(obj)
	$(ARCHIVER) $(ARFLAGS) $(ATLASlib) $(obj)
	$(RANLIB) $(ATLASlib)
	touch cnrm2.grd

ATL_cnrm2.o : ATL_cnrm2.c
	$(ICC) $(ICCFLAGS) -DSCPLX -o $@ -c ATL_cnrm2.c
ATL_cnrm2_xp0yp0aXbX.o : ATL_cnrm2_xp0yp0aXbX.c
	/usr/bin/x86_64-redhat-linux-gcc -O -fomit-frame-pointer -mavx -m64 -fPIC -m64 -fPIC -fomit-frame-pointer -mfpmath=387 -O2 $(CDEFS) -DSCPLX -o $@ -c ATL_cnrm2_xp0yp0aXbX.c
